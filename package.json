{
	"name": "anitrack",
	"version": "1.0.0",
	"description": "Watch and Track on AniTrack",
	"author": "zetharionn",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"lint": "pnpm lint:linter && pnpm lint:formatter",
		"lint:linter": "biome lint --write ./src/*.*",
		"lint:formatter": "biome format --write ./src/*.*",
		"lint:steiger": "steiger ./src",
		"test": "vitest run",
		"test:watch": "vitest watch",
		"build": "tsc -b && vite build",
		"preview": "vite preview",
		"prepare": "husky"
	},
	"packageManager": "pnpm@9.9.0",
	"homepage": "https://github.com/zetharionn/anitrack",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/zetharionn/anitrack"
	},
	"bugs": {
		"url": "https://github.com/zetharionn/anitrack/issues"
	},
	"keywords": [
		"react",
		"tracker",
		"typescript",
		"anime",
		"zustand",
		"anitrack"
	],
	"dependencies": {
		"@hookform/resolvers": "3.9.0",
		"@nextui-org/react": "2.4.8",
		"@supabase/supabase-js": "2.45.4",
		"@vercel/analytics": "1.3.1",
		"@vercel/speed-insights": "1.0.12",
		"classnames": "2.5.1",
		"framer-motion": "11.5.6",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"react-hook-form": "7.53.0",
		"react-router-dom": "6.26.2",
		"zod": "3.23.8",
		"zustand": "4.5.5"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.2",
		"@commitlint/cli": "19.5.0",
		"@commitlint/config-conventional": "19.5.0",
		"@feature-sliced/eslint-config": "0.1.1",
		"@types/node": "22.5.5",
		"@types/react": "18.3.8",
		"@types/react-dom": "18.3.0",
		"@typescript-eslint/eslint-plugin": "8.6.0",
		"@typescript-eslint/parser": "8.6.0",
		"@vitejs/plugin-react-swc": "3.7.0",
		"autoprefixer": "10.4.20",
		"dotenv": "16.4.5",
		"husky": "9.1.6",
		"lint-staged": "15.2.10",
		"postcss": "8.4.47",
		"sass": "1.79.3",
		"steiger": "0.4.0",
		"tailwindcss": "3.4.12",
		"typescript": "5.6.2",
		"vite": "5.4.7",
		"vitest": "2.1.1"
	}
}
